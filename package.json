{
  "name": "RnBlueRise",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "clean-android": "cd android && ./gradlew clean && cd ..",
    "clean-ios": "cd ios && xcodebuild clean && cd ..",
    "start": "react-native start",
    "android:dev:win": "set ENVFILE=env/.dev && npx react-native run-android --mode=devdebug",
    "android:dev:release:win": "set ENVFILE=env/.dev && yarn clean-android && npx react-native run-android --mode=devrelease",
    "android:prodDebug:win": "set ENVFILE=env/.prod && yarn clean-android && npx react-native run-android --mode=prodDebug",
    "android:dev:mac": "ENVFILE=env/.dev && npx react-native run-android --mode=devdebug",
    "android:dev:release:mac": "ENVFILE=env/.dev && yarn clean-android && npx react-native run-android --mode=devrelease",
    "android:prodDebug:mac": "ENVFILE=env/.prod && yarn clean-android && npx react-native run-android --mode=prodDebug",
    "ios:dev": "npx react-native run-ios --scheme RnBlueRise-Debug",
    "ios:prod": "yarn clean-ios && npx react-native run-ios --scheme RnBlueRise",
    "test": "jest"
  },
  "dependencies": {
    "@d11/react-native-fast-image": "^8.10.0",
